{
  "name": "@vanblog/server",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:noweb": "VANBLOG_DISABLE_WEBSITE=true nest start",
    "dev": "nest start --watch",
    "dev:noweb": "VANBLOG_DISABLE_WEBSITE=true nest start --watch",
    "start:dev": "nest start --watch",
    "start:dev:noweb": "VANBLOG_DISABLE_WEBSITE=true nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^9.4.3",
    "@nestjs/core": "^9.4.3",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mongoose": "^9.2.2",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.4.3",
    "@nestjs/schedule": "^2.2.3",
    "@nestjs/swagger": "^6.3.0",
    "@types/line-reader": "^0.0.34",
    "@types/request-ip": "^0.0.37",
    "axios": "^1.6.2",
    "cheerio": "1.0.0-rc.12",
    "compressing": "^1.10.0",
    "dayjs": "^1.11.10",
    "express": "^4.18.2",
    "feed": "^4.2.2",
    "highlight.js": "^11.9.0",
    "image-size": "^1.0.2",
    "jimp": "^0.22.10",
    "js-base64": "^3.7.5",
    "js-sha256": "^0.9.0",
    "line-reader": "^0.4.0",
    "lodash": "^4.17.21",
    "markdown-it": "^13.0.2",
    "markdown-it-katex": "^2.0.3",
    "markdown-it-task-lists": "^2.1.1",
    "mongodb": "^5.9.1",
    "mongoose": "^7.6.6",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "picgo": "^1.5.6",
